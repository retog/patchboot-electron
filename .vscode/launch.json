  "version": "0.2.0",
  "configurations": [
    {
      "type": "electron",
      "request": "launch",
      "name": "Patchboot Electron default abb",
      "appDir": "${workspaceFolder}"
    },
    {
      "type": "electron",
      "request": "launch",
      "name": "Patchboot Electron ssb app",
      "appDir": "${workspaceFolder}",
      "env": {"ssb_appname": "ssb"}
    },
    {
      "type": "electron",
      "request": "launch",
      "name": "Patchboot Electron ssb-test app",
      "appDir": "${workspaceFolder}",
      "env": {"ssb_appname": "ssb-test"}
    }
  ]
}